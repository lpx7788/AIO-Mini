page {
  background: #fff;
}

.headTit {
  display: flex;
  justify-content: space-between;
  height:96rpx;
  border-top: 1px solid #F6F6F6;
  align-items: center;
  padding: 0 30rpx;
  background: #F6F6F6;
  font-size: 30rpx;
}

.headTit .mainTitle {
  color: #222;
  position: relative;
  padding-left: 20rpx;
  align-items: center;

}

.headTit .mainTitle .manageTit{
  height: 30rpx;
   position: relative;
   padding-left:10rpx;
  font-size: 32rpx;

}

.headTit .mainTitle .manageTit:before {
  content: '';
  display: block;
  width: 8rpx;
  height: 32rpx;
  background: #4d65fd;
  position: absolute;
  left:-10rpx;
  top: 50%;
  margin-top: -16rpx;
}

